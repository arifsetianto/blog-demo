{
    "name": "pandawa/skeleton",
    "description": "The higher laravel framework for enterprise application.",
    "keywords": ["framework", "pandawa"],
    "license": "MIT",
    "type": "project",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "^4.1",
        "pandawa/pandawa": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "^1.9@dev",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^9.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/laravel",
            "Blog\\Api\\": "app/blog/Api",
            "Blog\\Web\\": "app/blog/Web",
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "symfony/thanks": true
        }
    }
}
